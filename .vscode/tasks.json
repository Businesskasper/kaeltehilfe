{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build backend",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/kaeltebus-backend/src/kaeltebus-backend.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"label": "build backend test",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/kaeltebus-backend/test/kaeltebus-test.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary"
			],
			"problemMatcher": "$msCompile"
		}
	]
}
